{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "comma-dangle": "off",
    "consistent-return": "off",
    "no-console": "off",
    "no-multiple-empty-lines": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
